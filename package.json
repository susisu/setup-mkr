{
  "private": true,
  "name": "@susisu/setup-mkr",
  "version": "3.0.1",
  "description": "Setup mkr - CLI for Mackerel",
  "keywords": [
    "actions",
    "setup",
    "mkr",
    "mackerel"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/susisu/setup-mkr.git"
  },
  "author": "Susisu <susisu2413@yahoo.co.jp>",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "format": "prettier --write '*.js' src",
    "format:check": "prettier --check '*.js' src",
    "lint": "eslint --fix '*.js' src",
    "lint:check": "eslint '*.js' src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "jest",
    "build": "ncc build src/index.ts --source-map --license licenses.txt",
    "clean": "rimraf dist",
    "all": "run-s clean format:check lint:check test build"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/tool-cache": "^2.0.1"
  },
  "devDependencies": {
    "@susisu/eslint-config": "^0.0.72",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "3.1.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
